{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-unused-expression": true
    },
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "member-ordering": [
            false
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "no-console": false,
        "jsdoc-format": false
    },
    "rulesDirectory": []
}
