{
  "name": "supomation-core",
  "version": "0.0.0",
  "description": "supomation core",
  "license": "MIT",
  "repository": "patevs/supomation",
  "author": "PatEvs",
  "engines": {
    "node": ">=8"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "prelint": "prettier --check --write src/**/*.js && echo. && echo Prettier Completed!",
    "lint": "eslint --fix src/**/*.js && echo Lint Completed!",
    "test": "jest && echo. && echo Test Complete!"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "listr": "^0.14.3",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true
  }
}
