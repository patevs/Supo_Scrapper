{
    "name": "supomation",
    "version": "0.4.0",
    "description": "Supomation CLI",
    "license": "MIT",
    "repository": "https://github.com/patevs/supomation",
    "author": "PatEvs",
    "files": [
        "lib"
    ],
    "main": "lib/index",
    "types": "lib/index",
    "scripts": {
        "clean": "rimraf lib && rimraf coverage && echo Clean Complete!",
        "clean:full": "npm run clean && trash node_modules docs && echo. && echo Full Clean Complete!",
        "gen:docs": "cd scripts && gen-docs.cmd",
        "start": "ts-node src/index.ts",
        "format": "prettier --write \"{src,src/__tests__}/**/*.ts\" --single-quote --trailing-comma es5",
        "lint": "tslint --force --format verbose \"src/**/*.ts\" && echo Lint Complete!",
        "prepublishOnly": "npm run build",
        "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
        "build": "tsc --pretty",
        "test": "jest && echo. && echo Test Complete!",
        "coverage": "jest --coverage",
        "watch": "npm run build -- --watch",
        "watch:test": "jest --watch"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "boxen": "^4.1.0",
        "chalk": "^2.4.2",
        "cheerio": "^1.0.0-rc.3",
        "clear": "^0.1.0",
        "dotenv": "^8.0.0",
        "log-symbols": "^3.0.0",
        "mongodb": "^3.3.0-beta2",
        "signale": "^1.4.0",
        "write-json-file": "^4.1.1"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.12",
        "@types/clear": "^0.1.0",
        "@types/dotenv": "^6.1.1",
        "@types/jest": "^24.0.15",
        "@types/mongodb": "^3.1.30",
        "@types/node": "^12.6.8",
        "@types/prettier": "^1.18.0",
        "@types/rimraf": "^2.0.2",
        "@types/signale": "^1.2.1",
        "coveralls": "^3.0.2",
        "jest": "^24.8.0",
        "prettier": "^1.14.3",
        "rimraf": "^2.6.2",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "typescript": "^3.1.1"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "jest": {
        "preset": "ts-jest"
    }
}
