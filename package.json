{
	"name": "supomation",
	"version": "0.3.0",
	"description": "Supomation CLI",
	"author": "PatEvs <ppevans11@gmail.com> (https://github.com/patevs)",
	"repository": "patevs/supomation",
	"license": "MIT",
	"files": [
		"src"
	],
	"main": "src/index.js",
	"engines": {
		"npm": ">= 10.0.0"
	},
	"scripts": {
		"clean": "trash node_modules",
		"start": "node src/index.js",
		"pretest": "eslint src/ --fix && echo Pretest Complete!",
		"test": "jest && echo Test Complete! && exit 0"
	},
	"devDependencies": {
		"eslint": "^6.1.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-config-xo": "^0.26.0",
		"eslint-plugin-prettier": "^3.1.0",
		"husky": "^3.0.1",
		"jest": "^24.8.0",
		"prettier": "^1.18.2"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.ts": [
			"eslint --fix",
			"git add"
		],
		"*.json": [
			"prettier --write",
			"git add"
		]
	},
	"eslintConfig": {
		"extends": [
			"xo",
			"prettier"
		],
		"env": {
			"jest": true,
			"node": true
		},
		"plugins": [
			"prettier"
		],
		"rules": {
			"valid-jsdoc": 0,
			"no-warning-comments": 0
		}
	},
	"jest": {
		"testEnvironment": "node"
	}
}
