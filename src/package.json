{
    "name": "supomation-api",
    "version": "0.0.1",
    "description": "Supomation API",
    "author": "PatEvs <ppevans11@gmail.com> (https://github.com/patevs)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/patevs/supomation.git"
    },
    "engines": {
        "node": ">=10.0.0",
        "npm": ">=6.0.0"
    },
    "scripts": {
        "develop": "strapi develop",
        "start": "strapi start",
        "build": "strapi build",
        "strapi": "strapi",
        "prettier": "prettier --check --write src/**/*.js && echo. && echo Prettier Completed!",
        "lint": "npm run prettier && eslint --fix **/*.js && echo. && echo Lint Completed!"
    },
    "dependencies": {
        "lodash": "^4.17.5",
        "strapi": "3.0.0-beta.15",
        "strapi-admin": "3.0.0-beta.15",
        "strapi-utils": "3.0.0-beta.15",
        "strapi-plugin-settings-manager": "3.0.0-beta.15",
        "strapi-plugin-content-type-builder": "3.0.0-beta.15",
        "strapi-plugin-content-manager": "3.0.0-beta.15",
        "strapi-plugin-users-permissions": "3.0.0-beta.15",
        "strapi-plugin-email": "3.0.0-beta.15",
        "strapi-plugin-upload": "3.0.0-beta.15",
        "strapi-hook-bookshelf": "3.0.0-beta.15",
        "strapi-hook-knex": "3.0.0-beta.15",
        "knex": "latest",
        "sqlite3": "latest"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.0",
        "eslint": "^6.2.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.0",
        "prettier": "^1.18.2"
    },
    "strapi": {
        "uuid": "99dd530e-ffc4-4002-8a9a-3de772f3a2ea"
    },
    "eslintConfig": {
        "env": {
            "es6": true,
            "node": true,
            "jest": true
        },
        "extends": [
            "eslint:recommended",
            "prettier"
        ],
        "parserOptions": {
            "ecmaVersion": 2018
        },
        "plugins": [
            "prettier"
        ],
        "rules": {
            "quotes": [
                "error",
                "single"
            ],
            "semi": [
                "error",
                "always"
            ]
        }
    },
    "prettier": {
        "singleQuote": true,
        "semi": true
    }
}
