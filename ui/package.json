{
  "name": "supomation-ui",
  "description": "Supomation UI Dashboard",
  "version": "0.0.1",
  "author": "PatEvs <ppevans11@gmail.com> (https://github.com/patevs)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/supomation.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prettier": "prettier --check --write **/*.js && echo. && echo Prettier Completed!",
    "lint": "npm run prettier && eslint --fix **/*.js && echo. && echo Lint Completed!",
    "test": "react-scripts test"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "prettier",
      "react-app"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
