{
  "name": "supomation-ui",
  "description": "Supomation UI Dashboard",
  "version": "0.0.1",
  "author": "PatEvs <ppevans11@gmail.com> (https://github.com/patevs)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/supomation.git"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prettier": "prettier --check --write **/*.js && echo. && echo Prettier Completed!",
    "lint": "npm run prettier && eslint --fix **/*.js && echo. && echo Lint Completed!",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "classnames": "^2.2.6",
    "history": "^4.9.0",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "prettier"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
