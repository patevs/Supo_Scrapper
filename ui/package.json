{
  "name": "supomation-ui",
  "version": "0.0.1",
  "description": "Supomation UI Dashboard based on Material Dashboard by Creative Tim",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/supomation-dash.git"
  },
  "bugs": {
    "url": "https://github.com/patevs/supomation-dash/issues"
  },
  "homepage": "https://github.com/patevs/supomation-dash",
  "main": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prettier": "prettier --check --write **/*.js **/*.jsx && echo. && echo Prettier Completed!",
    "lint": "npm run prettier && eslint --fix **/*.js **/*.jsx && echo. && echo Lint Completed!"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "chartist": "^0.11.3",
    "classnames": "2.2.6",
    "history": "4.9.0",
    "perfect-scrollbar": "1.4.0",
    "prop-types": "15.7.2",
    "react": "^16.9.0",
    "react-chartist": "0.13.3",
    "react-dom": "^16.9.0",
    "react-google-maps": "9.4.5",
    "react-router-dom": "5.0.1",
    "react-scripts": "^3.1.1",
    "react-swipeable-views": "0.13.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "1.18.2"
  },
  "optionalDependencies": {
    "@types/googlemaps": "3.36.4",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "6.10.0",
    "typescript": "3.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": true
  }
}
